{describe, it, expect, to_equal} = import '@fink/jest'
{Math} = import '@fink/js-interop/globals'

{π, clamp} = import './math'


describe 'math', fn:
  it 'provides math funcs', fn:
    expect
      π
      to_equal Math.PI


  it 'clamp', fn:
    expect
      clamp 0, -1, 2
      to_equal 0

    expect
      clamp 0, 3, 2
      to_equal 2

