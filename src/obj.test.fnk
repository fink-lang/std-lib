{describe, it, expect, to_equal} = import '@fink/jest'

{
  obj_keys, obj_values, obj_key_values, key_values_to_obj, obj_has
} = import './obj'


describe 'objects', fn:
  it 'has key', fn:
    expect
      obj_has {foo: 1, 'bar spam': 2}, 'foo'
      to_equal
        true

    expect
      obj_has {}, 'toString'
      to_equal
        false

  it 'gets keys', fn:
    expect
      obj_keys {foo: 1, 'bar spam': 2}
      to_equal
        ['foo', 'bar spam']


  it 'gets values', fn:
    expect
      obj_values {foo: 1, 'bar spam': 2}
      to_equal
        [1, 2]


  it 'gets key,values', fn:
    expect
      obj_key_values {foo: 1, 'bar spam': 2}
      to_equal list:
        ['foo', 1]
        ['bar spam', 2]


  it 'creates obj from key,values', fn:
    expect
      key_values_to_obj list:
        ['foo', 1]
        ['bar spam', 2]
      to_equal
        {foo: 1, 'bar spam': 2}
