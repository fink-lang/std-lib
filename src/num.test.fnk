{describe, it, expect, to_equal} = import '@fink/jest'

{parse_int, parse_float} = import './num'


describe 'numbers', fn:
  it 'parses integer', fn:
    expect
      parse_int '1234'
      to_equal
        1234

  it 'parses floats', fn:
    expect
      parse_float '1234.567'
      to_equal
        1234.567
