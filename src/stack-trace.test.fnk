{describe, it, expect, to_equal} = import '@fink/jest'

{replace_all, split} = import './regex'

{stack_trace} = import './stack-trace'


describe 'stack_trace', fn:
  it 'gets stack trace', fn:
    foo = fn:
      stack = stack_trace _
      [, line] = pipe stack:
        replace_all ?, rx/\(.+\//, '('
        split ?, '\n'
      line

    expect
      foo _
      to_equal
        '    at foo (stack-trace.test.fnk:11:15)'